{
    "name": "mrgoodbytes8667/twitch-response-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle for Twitch API Response objects and enums",
    "license": "CC-BY-NC-4.0",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "mrgoodbytes8667/response-bundle": "^1.2",
        "symfony/config": "^5.2",
        "symfony/console": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/framework-bundle": "^5.2",
        "symfony/messenger": "^5.2",
        "symfony/serializer": "^5.2",
        "symfony/string": "^5.2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.2",
        "symfony/var-dumper": "^5.2"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@goodbytes.live"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "spatie/enum": "<3.7.2",
        "symfony/serializer": "<5.2.2"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\TwitchResponseBundle\\": "src/",
            "Bytes\\TwitchResponseBundle\\Tests\\": "Tests/"
        }
    }
}
