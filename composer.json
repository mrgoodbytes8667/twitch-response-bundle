{
    "name": "mrgoodbytes8667/twitch-response-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle for Twitch API response objects, enums, and signature verification",
    "license": "CC-BY-NC-4.0",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "illuminate/support": "^8.0",
        "mrgoodbytes8667/response-bundle": "^2.0.9",
        "symfony/config": "^5.3",
        "symfony/console": "^5.3",
        "symfony/dependency-injection": "^5.3",
        "symfony/framework-bundle": "^5.3",
        "symfony/messenger": "^5.3",
        "symfony/serializer": "^5.3",
        "symfony/string": "^5.3",
        "symfony/validator": "^5.3"
    },
    "require-dev": {
        "doctrine/common": "^3.0",
        "fakerphp/faker": "^1.13",
        "mrgoodbytes8667/test-common": ">=0.0.18",
        "mrgoodbytes8667/test-common-faker": "^0.3.3",
        "mrgoodbytes8667/test-twitch-faker": ">=0.0.1",
        "symfony/test-pack": "^1.0.8",
        "symfony/var-dumper": "^5.3"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@goodbytes.live"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "doctrine/annotations": "<1.11.1",
        "doctrine/cache": "<1.10.2",
        "doctrine/collections": "<1.6.7",
        "doctrine/event-manager": "<1.1.1",
        "doctrine/inflector": "<1.4.3",
        "doctrine/lexer": "<1.2.1",
        "doctrine/persistence": "<2.1.0",
        "phpunit/phpunit": "<9.5",
        "spatie/enum": "<3.7.2",
        "symfony/property-info": "<5.3",
        "symfony/serializer": "<5.2.2"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\TwitchResponseBundle\\": "src/",
            "Bytes\\TwitchResponseBundle\\Tests\\": "Tests/"
        }
    }
}
