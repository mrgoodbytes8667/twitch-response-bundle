{
    "name": "mrgoodbytes8667/twitch-response-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle for Twitch API response objects, enums, and signature verification",
    "license": "CC-BY-NC-4.0",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "doctrine/annotations": "^1.0 | ^2.0",
        "mrgoodbytes8667/response-bundle": "^5.0 | ^6.0",
        "symfony/messenger": "^5.3 | ^6.0",
        "symfony/validator": "^5.3 | ^6.0"
    },
    "require-dev": {
        "doctrine/common": "^3.0",
        "fakerphp/faker": "^1.13",
        "mrgoodbytes8667/test-common": ">=0.1",
        "mrgoodbytes8667/test-common-faker": "^0.3.5",
        "mrgoodbytes8667/test-enum-faker": ">=0.0.1",
        "mrgoodbytes8667/test-twitch-faker": ">=0.0.5",
        "rector/rector": "^0.13 | ^0.14 | ^0.15",
        "symfony/test-pack": "^1.0.8",
        "symfony/var-dumper": "^5.3 | ^6.0"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@mrgoodbytes.dev"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "doctrine/annotations": "<1.11.1",
        "doctrine/cache": "<1.10.2",
        "doctrine/collections": "<1.6.7",
        "doctrine/event-manager": "<1.1.1",
        "doctrine/inflector": "<1.4.3",
        "doctrine/lexer": "<1.2.1",
        "doctrine/persistence": "<2.1.0",
        "phpunit/phpunit": "<9.5 | 9.5.8 | >=10",
        "symfony/property-info": "<5.3",
        "symfony/serializer": "<5.2.2"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\TwitchResponseBundle\\": "src/",
            "Bytes\\TwitchResponseBundle\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev"
}
